// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "editor.tabSize": 2,
  "files.associations": {
    "*.vue": "vue"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "emmet.syntaxProfiles": {
    "vue-html": "html",
    "vue": "html"
  },
  "emmet.useNewEmmet": false,
  "[css]": {
    "editor.quickSuggestions": true
  },
  "[sass]": {
    "editor.quickSuggestions": true
  },
  "[less]": {
    "editor.quickSuggestions": true
  }
}
